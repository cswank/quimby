{{define "content"}}
  <div id="content">
    {{.Gadget.Name}}
    {{range $location, $devices := .Gadget.Status}}
      <div>{{$location}}</div>
      <div>
        {{range $key, $device := $devices}}
          {{ template "device" $device}}
        {{end}}
      </div>
    {{end}}
  </div>
{{end}}

